<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:AvaloniaTreeTabUiTest.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaTreeTabUiTest.Views.MainView">
    <Design.DataContext>
        <viewModels:MainViewModel/>
    </Design.DataContext>
    <Grid>
        <StackPanel Width="150">
            <TextBlock Text="MainPage" TextAlignment="Center"/>
            <Button Content="Справочники" Margin="2" Command="{Binding RefBooksCmd}"/>
            <Button Content="СТСиК" Margin="2" Command="{Binding STSiKCmd}"/>
            <Button Content="СУб" Margin="2" Command="{Binding SMSCmd}"/>
        </StackPanel>
    </Grid>
</UserControl>
